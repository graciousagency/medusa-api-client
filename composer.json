{
    "name": "gracious/medusa-api-client",
    "description": "This package provides Jane-PHP generated API models and client based on the OpenAPI specification for MedusaJS",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Gracious\\MedusaApiClientBundle\\": ["src/","generated/"]
        }
    },
    "authors": [
        {
            "name": "Ash",
            "email": "a.grlicka@gracious.nl"
        }
    ],
    "require": {
        "php": ">=8.3",
        "jane-php/open-api-runtime": "^7.3",
        "symfony/serializer": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.15",
        "jane-php/open-api-2": "^7.5",
        "jane-php/open-api-3": "^7.9",
        "cweagans/composer-patches": "~1.0"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": false,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        },
        "patches": {
          "jane-php/open-api-3": {
            "Fix issue with processing medusajs schema": "patches/jane_schema_gen_fix.patch"
          }
        }
    }
}
